{
  "manifest_version": 2,
  "name": "Bookmark Manager",
  "version": "1.0",
  "description": "A simple bookmark manager for Chrome.",
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icons/bookmark32.png"
  },
  "icons": {
    "16": "icons/bookmark16.png",
    "32": "icons/bookmark32.png",
    "64": "icons/bookmark64.png",
    "128": "icons/bookmark128.png"
  },
  "permissions": [
    "tabs",
    "bookmarks"
  ]
}